{
    "name": "tinect/redirects",
    "description": "Redirect plugin for Shopware 6",
    "version": "1.1.0",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "authors": [
      {
        "name": "Tinect"
      }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "shopware/core": "~6.5"
    },
    "extra": {
        "shopware-plugin-class": "Tinect\\Redirects\\TinectRedirects",
        "label": {
            "de-DE": "Redirect Plugin",
            "en-GB": "Redirect plugin"
        }
    },
    "autoload": {
        "psr-4": {
            "Tinect\\Redirects\\": "src/"
        }
    },
    "scripts": {
        "cs-fix": "docker run --rm -v $(pwd):$(pwd) -w $(pwd) oskarstark/php-cs-fixer-ga --diff",
        "phpstan": "docker run --rm -v $(pwd):/app aragon999/phpstan-shopware:v6.5.7 analyse ."
    }
}
